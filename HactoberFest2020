#include <iostream>
#include <math.h>
#include <algorithm>
// g++ hello.cpp && ./a.out
using namespace std;

int main () {
	int T;
	long d;
	cin >> T;
	cin >> d;
	int count = 0;
	while(T--) {
		int x, y;
		cin >> x >> y;
		long ans = ((x * x) + (y*y));
		//cout << ans << endl;
		if (ans <= d*d) {
			count++;
		}
	}
	cout <<count <<endl;
	return 0;
}
